<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingConstraints"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tv_titleOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我是读取assets资源svg"
        android:textColor="@color/black"
        android:textSize="18sp"
        />

    <WebView
        android:id="@+id/wb_viewOne"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:layout_constraintTop_toBottomOf="@id/tv_titleOne"/>


    <TextView
        android:id="@+id/tv_titleTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/wb_viewOne"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:text="我是修改后的svg(修改口罩为白色)"/>




    <WebView
        android:id="@+id/wb_viewTwo"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:layout_constraintTop_toBottomOf="@id/tv_titleTwo"/>
</androidx.constraintlayout.widget.ConstraintLayout>